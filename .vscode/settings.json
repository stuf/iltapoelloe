// Place your settings in this file to overwrite default and user settings.
{
  "javascript.validate.enable": false,
  "files.associations": {
    "**/*.js.flow": "javascript"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node/modules/*": true,
    "**/flow-typed/*": true
  }
}
